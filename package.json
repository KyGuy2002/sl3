{
  "name": "sl3",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro build && wrangler pages dev",
    "astro": "astro",
    "find-local-db": "find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit",
    "db:update": "npx drizzle-kit generate | tee output.log | grep -q 'No schema changes' && echo '=== No changes!' && exit 0; cat ./drizzle/$(ls ./drizzle | tail -2 | head -1) > schema.sql; npx wrangler d1 execute DB --file 'schema.sql'"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.2.1",
    "@astrojs/react": "^4.1.6",
    "@astrojs/tailwind": "^5.1.5",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-hover-card": "^1.1.5",
    "@radix-ui/react-slot": "^1.1.1",
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/server": "^13.1.0",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "astro": "^5.1.8",
    "better-sqlite3": "^11.8.1",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.4",
    "lucide-react": "^0.473.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250121.0",
    "drizzle-kit": "^0.30.2",
    "tsx": "^4.19.2",
    "wrangler": "^3.105.0"
  }
}
